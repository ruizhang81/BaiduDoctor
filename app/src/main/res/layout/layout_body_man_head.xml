<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/body_root"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:src="@drawable/man_bigface" />

    <ImageView
        android:id="@+id/intelligence_highlight_0101"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/imageView"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="0dp"
        android:src="@drawable/intelligence_highlight_0101" />


    <ImageView
        android:id="@+id/intelligence_highlight_0105"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/intelligence_highlight_0101"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="-10dp"
        android:src="@drawable/intelligence_highlight_0105" />

    <ImageView
        android:id="@+id/intelligence_highlight_0102"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/intelligence_highlight_0101"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="-10dp"
        android:src="@drawable/intelligence_highlight_0102" />


    <ImageView
        android:id="@+id/intelligence_highlight_0103"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/intelligence_highlight_0102"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="-30dp"
        android:src="@drawable/intelligence_highlight_0103" />

    <ImageView
        android:id="@+id/intelligence_highlight_0106"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/intelligence_highlight_0102"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="-10dp"
        android:src="@drawable/intelligence_highlight_0106" />

    <ImageView
        android:id="@+id/intelligence_highlight_male_0104"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/intelligence_highlight_0103"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="5dp"
        android:src="@drawable/intelligence_highlight_male_0104" />

</RelativeLayout>
